---
# {{ ansible_managed }}
rexray:
  logLevel: warn
  osDrivers:
{% for driver in rexray_os_drivers %}
  - {{ driver }}
{% endfor %}
  volumeDrivers:
{% for driver in rexray_volume_drivers %}
  - {{ driver }}
{% endfor %}
  storageDrivers:
{% for driver in rexray_storage_drivers %}
  - {{ driver }}
{% endfor %}
